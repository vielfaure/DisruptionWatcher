{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
      "max-line-length": [true, { "limit": 120, "ignore-pattern": "^import .* from |^export | implements " }],
      "ordered-imports": [false],
      "ignoreStrings": true,
      "ignoreUrls": true
    },
    "rulesDirectory": []
  }
  